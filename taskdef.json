{
    "executionRoleArn": "arn:aws:iam::815719488391:role/ecsTaskexecution",
    "containerDefinitions": [
        {
            "name": "myapp",
            "image": "815719488391.dkr.ecr.us-east-1.amazonaws.com/python-repo:latest",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 0,
                    "protocol": "tcp",
                    "containerPort": 5000
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "networkMode": "bridge",
    "cpu": "256",
    "memory": "512",
    "family": "python"
}
