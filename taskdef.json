{
    "taskRoleArn": "arn:aws:iam::522589518420:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "myapp",
            "image": "praneshprashar25/python_app_sql:latest",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 5000,
                    "protocol": "tcp",
                    "containerPort": 5000
                }
            ]
        }
    ],
    "environment": [
        {
          "name": "DB_HOST",
          "value": "db"
        },
        {
          "name": "DB_NAME",
          "value": "mysql"
        },
        {
          "name": "DB_PASSWORD",
          "value": "password"
        },
        {
          "name": "DB_USER",
          "value": "root"
        }
      ],
    "requiresCompatibilities": [
        "EC2"
    ]
}
