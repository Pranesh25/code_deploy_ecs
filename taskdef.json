{
    "executionRoleArn": "arn:aws:iam::306576226437:role/ecsTaskExecutionRole",
    "family": "python-task",
    "containerDefinitions": [
        {
            "name": "myapp",
            "image": "306576226437.dkr.ecr.us-east-1.amazonaws.com/python-repo:latest",
            "essential": true,
            "memory": 400,
            "memoryReservation": 128,
            "portMappings": [
                {
                    "hostPort": 5000,
                    "protocol": "tcp",
                    "containerPort": 5000
                }
            ]
        }
    ],
    "environment": [
        {
          "name": "DB_HOST",
          "value": "db"
        },
        {
          "name": "DB_NAME",
          "value": "mysql"
        },
        {
          "name": "DB_PASSWORD",
          "value": "password"
        },
        {
          "name": "DB_USER",
          "value": "root"
        }
      ],
    "requiresCompatibilities": [
        "EC2"
    ]
}
