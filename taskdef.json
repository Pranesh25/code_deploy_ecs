{
    "executionRoleArn": "arn:aws:iam::404797016134:instance-profile/task-execution-policy",
    "containerDefinitions": [
        {
            "name": "myapp",
            "image": "praneshprashar25/python_app_sql:latest",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 0,
                    "protocol": "tcp",
                    "containerPort": 5000
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "python"
}
