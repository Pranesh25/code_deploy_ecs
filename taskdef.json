{
    "executionRoleArn": "arn:aws:iam::315105850655:instance-profile/ec2-role-admin",
    "containerDefinitions": [
        {
            "name": "myapp",
            "image": "315105850655.dkr.ecr.us-east-1.amazonaws.com/pyhton-repo:latest",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 0,
                    "protocol": "tcp",
                    "containerPort": 5000
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "python"
}
